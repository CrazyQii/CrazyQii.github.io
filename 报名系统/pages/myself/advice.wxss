/* pages/myself/advice.wxss */
page {
	background: url("data:image/jpg;base64,") no-repeat;
  	background-size: cover;
}

/*文本域模块*/
.text_content {
	width: 100%;
	height: 400rpx;

}

.connect view,
.text_content view {
	margin-top: 20rpx;
	margin-left: 50rpx;
	padding-top: 20rpx;
	margin-bottom: 10rpx;
}

.text_content textarea {
	width: 95%;
	height: 80%;
	margin: 0 auto;
	padding-left: 20rpx;
	line-height: 20rpx;
	border: 1rpx solid #919191;
}

/*联系模块*/
.connect {
	width: 100%;
	margin-bottom: 40rpx;
}

.connect input {
	width: 100%;
	height: 80rpx;
	padding-left: 50rpx;
	border-top: 1rpx solid #919191;
	border-bottom: 1rpx solid #919191;
}

button {
	width: 90%;
}